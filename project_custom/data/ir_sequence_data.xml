<odoo>
    <data noupdate="1">
         <!-- sequence for Billing Statement -->
        <record id="sequence_billing_statement" model="ir.sequence">
            <field name="name">Billing Statement</field>
            <field name="code">billing.statement</field>
            <field name="prefix">BS_%(year)s-</field>
            <field name="padding">5</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

        <!-- sequence for Project -->
        <record id="sequence_project_project" model="ir.sequence">
            <field name="name">Project</field>
            <field name="code">project.project</field>
            <field name="prefix">PC_%(year)s-</field>
            <field name="padding">5</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

        <record id="seq_project_boq" model="ir.sequence">
            <field name="name">Project BOQ Sequence</field>
            <field name="code">project.boq</field>
            <field name="prefix">B</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>